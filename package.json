{
  "name": "vue-tailwindcss-atomic-design",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run tailwind:css && vue-cli-service serve",
    "build": "npm run tailwind:css && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "tailwind:css": "./node_modules/.bin/tailwind build ./src/assets/css/main.css -c ./tailwind.config.js -o ./public/css/styles.css",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "tailwindcss": "^1.1.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.18.2",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
